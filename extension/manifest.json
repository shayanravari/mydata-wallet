{
  "manifest_version": 3,
  "name": "MyData Wallet (Alpha)",
  "version": "0.0.1",
  "description": "Capture and manage your data.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
  "http://127.0.0.1:8000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}